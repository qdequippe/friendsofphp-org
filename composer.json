{
    "name": "allfriendsofphp/allfriendsofphp",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "nette/utils": "^2.5",
        "symfony/var-dumper": "^4.1",
        "symplify/statie": "^5.1",
        "symplify/package-builder": "^5.1",
        "rinvex/country": "^3.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.3",
        "phpunit/phpunit": "^7.3",
        "symplify/easy-coding-standard": "^5.1",
        "symplify/monorepo-builder": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Fop\\": "src",
            "Fop\\MeetupCom\\": "packages/MeetupCom/src",
            "Fop\\PhpUg\\": "packages/PhpUg/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fop\\PhpUg\\Tests\\": "packages/PhpUg/tests",
            "Fop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check bin src tests packages",
        "fix-cs": "vendor/bin/ecs check bin src tests packages --fix",
        "phpstan": "vendor/bin/phpstan analyse bin src tests packages --level max",
        "generate": [
            "bin/console import-user-groups",
            "bin/console search-and-import-php-groups",
            "bin/console import-meetups",
            "vendor/bin/statie generate source"
        ]
    }
}
